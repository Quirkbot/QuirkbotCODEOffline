{
  "name": "quirkbot-code-lite",
  "executable-name": "Quirkbot",
  "version": "0.0.3",
  "description": "",
  "main": "main.html",
  "node-main": "main.js",
  "window": {
    "title": "Quirkbot",
    "toolbar": true,
    "frame": true,
    "width": 1920,
    "height": 1080,
    "min_width": 940,
    "min_height": 600
  },
  "externally_connectable": {
    "matches": [
        "*://localhost/*"
    ]
  },
  "dependencies": {
    "quirkbot-data-api": "*",
    "quirkbot-chrome-app": "*",
    "quirkbot-code-static": "*",
    "quirkbot-compiler": "*"
  },
	"manifestUrl": "http://localhost:8080/package.json",
  "packages": {
      "mac": {
         "url": "http://localhost:8080/releases/mac/latest.zip"
      },
      "win": {
         "url": "http://localhost:8080/releases/win/latest.zip"
      },
      "linux64": {
         "url": "http://localhost:8080/releases/linux/latest.tar.gz"
      }
  },
